{
  "name": "@fluentdoc/avataaars",
  "version": "1.0.2",
  "description": "Custom fork of Avataaars for FluentDoc with worldbuilding extensions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickgaston-fd/avataaars"
  },
  "author": "Patrick Gaston",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "react",
    "avatar"
  ],
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "import-sort-style-renke": "^4.2.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "lodash": ">=4.17.21",
    "prop-types": "^15.7.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-renke"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-renke"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  }
}
